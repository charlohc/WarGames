<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="scenePane" minHeight="600.0" minWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.ntnu.IDATT2001.charlohc.WarGames.Controllers.ViewCustomArmyTwo">
    <children>
        <Label layoutX="390.0" layoutY="25.0" text="View Army Two">
            <font>
                <Font name="System Bold" size="16.0" />
            </font></Label>
        <Button layoutX="45.0" layoutY="608.0" minHeight="40.0" minWidth="150.0" mnemonicParsing="false" onAction="#goBack" text="Go back">
            <font>
                <Font size="14.0" />
            </font></Button>
        <Button fx:id="chooseTerrain" layoutX="700.0" layoutY="608.0" minHeight="40.0" minWidth="150.0" mnemonicParsing="false" onAction="#chooseTerrain" text="Choose terrain">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button layoutX="375.0" layoutY="608.0" minHeight="40.0" minWidth="150.0" mnemonicParsing="false" onAction="#removeUnit" text="Remove">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Pane layoutX="102.0" layoutY="154.0" prefHeight="388.0" prefWidth="700.0">
            <children>
                <TableView fx:id="tableView" layoutX="80.0" layoutY="62.0" prefHeight="257.0" prefWidth="563.0">
                    <columns>
                        <TableColumn fx:id="typeColumn" prefWidth="208.19998168945312" text="Unit Type" />
                        <TableColumn fx:id="nameColumn" minWidth="0.0" prefWidth="202.199951171875" text="Name" />
                        <TableColumn fx:id="healthColumn" minWidth="0.0" prefWidth="152.800048828125" text="Health" />
                    </columns>
                </TableView>
                <Text fx:id="armyName" layoutX="80.0" layoutY="15.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-underline: true;">
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Text>
                <Pane fx:id="infoPane" layoutX="194.0" layoutY="348.0" minHeight="40.0" minWidth="303.0">
                    <children>
                        <Text fx:id="info" layoutX="25.0" layoutY="25.0" strokeType="OUTSIDE" strokeWidth="0.0">
                            <font>
                                <Font size="14.0" />
                            </font>
                        </Text>
                    </children></Pane>
            </children>
        </Pane>
        <Text layoutX="280.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Here you can se the units you have added to army two">
            <font>
                <Font size="14.0" />
            </font>
        </Text>
        <Text layoutX="300.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="you can remove the units, go back and add more">
            <font>
                <Font size="14.0" />
            </font>
        </Text>
    </children>
</Pane>
